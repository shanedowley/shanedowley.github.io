{
    "name": "shanedowley.github.io",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build:css": "npx tailwindcss -i ./src/input.css -o ./css/tailwind.css --minify",
        "watch:css": "npx tailwindcss -i ./src/input.css -o ./css/tailwind.css --watch --quiet",
        "serve:jekyll": "bundle exec jekyll serve --livereload --incremental --quiet",
        "dev": "concurrently -n CSS,JEKYLL -c magenta,cyan \"npm run watch:css\" \"npm run serve:jekyll\"",
        "build:jekyll": "JEKYLL_ENV=production bundle exec jekyll build --trace",
        "build:css:prod": "cross-env NODE_ENV=production npx tailwindcss -i ./src/input.css -o ./css/tailwind.css --minify",
        "build:prod": "npm-run-all build:css:prod build:jekyll"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shanedowley/shanedowley.github.io.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/shanedowley/shanedowley.github.io/issues"
    },
    "homepage": "https://github.com/shanedowley/shanedowley.github.io#readme",
    "devDependencies": {
        "autoprefixer": "^10.4.21",
        "concurrently": "^9.2.1",
        "cross-env": "^10.1.0",
        "cssnano": "^7.1.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.6",
        "tailwindcss": "^3.4.17"
    }
}
